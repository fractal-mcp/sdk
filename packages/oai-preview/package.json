{
  "name": "@fractal-mcp/oai-preview",
  "version": "1.0.0",
  "description": "React component for previewing OpenAI Apps SDK widgets in development",
  "repository": {
    "type": "git",
    "url": "https://github.com/fractal-mcp/sdk.git"
  },
  "type": "module",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "test": "jest",
    "release": "npm view \"$npm_package_name@$npm_package_version\" version >/dev/null 2>&1 && echo \"Skipping publish: $npm_package_name@$npm_package_version exists\" || npm publish --access public"
  },
  "files": [
    "dist",
    "src"
  ],
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/node": "^20.0.0",
    "@types/jest": "^29.0.0",
    "eslint": "^8.0.0",
    "typescript": "^5.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "playwright": "^1.40.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  }
}

