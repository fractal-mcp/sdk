{
  "name": "@fractal-mcp/mcp-ui-messenger",
  "version": "2.2.4",
  "description": "MCP-UI compatible cross-iframe messenger",
  "type": "module",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/fractal-mcp/sdk.git",
    "directory": "packages/mcp-ui-messenger"
  },
  "homepage": "https://github.com/fractal-mcp/sdk/tree/main/packages/mcp-ui-messenger/README.md",
  "bugs": {
    "url": "https://github.com/fractal-mcp/sdk/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "test": "echo 'no tests'",
    "release": "npm view \"$npm_package_name@$npm_package_version\" version >/dev/null 2>&1 && echo \"Skipping publish: $npm_package_name@$npm_package_version exists\" || npm publish --access public"
  },
  "files": [
    "dist",
    "src"
  ],
  "dependencies": {
    "@fractal-mcp/protocol": "*",
    "@fractal-mcp/shared-ui": "2.2.4"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "eslint": "^8.0.0",
    "typescript": "^5.0.0"
  }
}
