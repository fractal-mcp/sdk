{
  "name": "@fractal-mcp/cli",
  "version": "2.2.5",
  "description": "CLI tools for Fractal MCP development",
  "repository": {
    "type": "git",
    "url": "https://github.com/fractal-mcp/sdk.git"
  },
  "type": "module",
  "bin": {
    "fractal-mcp": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "release": "npm view \"$npm_package_name@$npm_package_version\" version >/dev/null 2>&1 && echo \"Skipping publish: $npm_package_name@$npm_package_version exists\" || npm publish --access public"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "dependencies": {
    "@fractal-mcp/bundle": "2.2.5",
    "commander": "^14.0.0",
    "chalk": "^5.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "eslint": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "fractal",
    "mcp",
    "cli",
    "bundler"
  ],
  "publishConfig": {
    "access": "public"
  }
}
