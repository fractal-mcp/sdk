{
  "name": "@fractal-mcp/mcp-express",
  "version": "1.0.0",
  "description": "Express server utilities for MCP server hosting",
  "repository": {
    "type": "git",
    "url": "https://github.com/fractal-mcp/sdk.git"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "test": "jest --passWithNoTests",
    "release": "npm view \"$npm_package_name@$npm_package_version\" version >/dev/null 2>&1 && echo \"Skipping publish: $npm_package_name@$npm_package_version exists\" || npm publish --access public"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@fractal-mcp/mcp": "*",
    "@modelcontextprotocol/sdk": "^1.0.0",
    "express": "^4.18.0",
    "jsonwebtoken": "^9.0.0",
    "jwk-to-pem": "^2.0.7"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "@types/jest": "^29.0.0",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/jwk-to-pem": "^2.0.0"
  }
} 