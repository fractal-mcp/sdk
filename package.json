{
  "name": "fractal-mcp-sdk",
  "version": "2.0.5",
  "description": "A monorepo managed with Turborepo",
  "repository": {
    "type": "git",
    "url": "https://github.com/fractal-mcp/sdk.git"
  },
  "private": true,
  "workspaces": [
    "apps/**",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "clean": "turbo run clean",
    "version:update": "node scripts/update-versions.js"
  },
  "devDependencies": {
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.0",
    "turbo": "^2.5.4"
  },
  "packageManager": "npm@10.0.0",
  "dependencies": {
    "commander": "^14.0.0",
    "vite-plugin-singlefile": "^2.3.0"
  }
}
